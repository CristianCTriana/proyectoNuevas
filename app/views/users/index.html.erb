<p id="notice"><%= notice %></p>
<%= @aviso %>
<%= form_tag(users_path, method: :get) do %>
<div>
  <%= label_tag :login %>
  <%= text_field_tag :login_email, params[:login_email] %>
</div>

<div>
  <%= label_tag :password %>
  <%= text_field_tag :login_password, params[:login_password] %>
</div>

<div>
  <%= submit_tag 'Login', name: nil %>
</div>
<% end %>
<br>
